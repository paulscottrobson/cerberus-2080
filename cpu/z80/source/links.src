#
# 		Handle DD/FD Shift
#

DD 		"[IX]" 0
		pIXY = &IX;
		ExecuteDDGroup();

FD 		"[IY]" 0
		pIXY = &IY;
		ExecuteDDGroup();
		
#
# 		Handle ED shift
#
ED 		"[ED]" 0
		ExecuteEDGroup();

#
# 		Handle CB shift
#
CB 		"[CB]" 0
		ExecuteCBGroup();

#
# 		Handle DDCB shift
#
DDCB 	"[DDCB]" 0
		ExecuteDDCBGroup();
